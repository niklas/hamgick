grammar Options

  rule options
    options_key [\s]*
    '=>' [\s]*
    (argument) [\s]*
  end

  rule options_key
    ':' [a-zA-Z_\d]+
  end

  rule arguments
    argument [\s]* "," [\s]* arguments
    /
    argument
  end

  rule argument
    fixnum
  end


  rule fixnum
    [+-]? [\d]+
  end

  rule string
    "'" [a-zA-Z]* "'"
  end

end
